<div class="question-container">
  <%= link_to experts_answer_path(answer), class:"no-underline", method: :get do%>
  <ul>
    <li class="title"><i class="fab fa-quora">.</i><%= answer.title %></li>
    <p class="timestamp">
      <%= answer.created_at.strftime("%Y/%m/%d %H:%M") %>
    </p>
  </ul>
  <% end %>
</div>